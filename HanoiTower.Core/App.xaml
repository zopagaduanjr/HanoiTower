<Application x:Class="HanoiTower.Core.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HanoiTower.Core"
             xmlns:models="clr-namespace:HanoiTower.Core.Models"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <local:Locator x:Key="VMLocator"/>
                    <models:DoubleToDurationConverter x:Key="DoubleToDurationConverter"/>
                    <models:ScaleConverter x:Key="ScaleConverter"></models:ScaleConverter>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
